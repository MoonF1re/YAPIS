    (local $temp2 i32)
    (local.get $elements)
    (global.set $my_tree)
    (global.get $my_tree)
    (call $balance)
    (global.set $balanced_result)
    (global.get $balanced_result)
    (return)
  )
  (func $process_queue (export "process_queue")
    (param $items i32)
