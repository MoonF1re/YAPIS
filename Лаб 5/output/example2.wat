    (local $temp i32)
    (local $elem_val i32)
    (local $temp2 i32)
    (local.get $items)
    (global.set $my_queue)
    (call $list_create (i32.const 0))
    (local.set $temp)
    (local.get $temp)
    (global.set $result)
    (i32.const 0)
    (global.set $i)
    (block $until_end_0
      (loop $until_start_0
    (global.get $i)
    (global.get $my_queue)
    (call $list_length)
    (i32.ge_s)
        (br_if $until_end_0)
    (global.get $my_queue)
    (global.get $i)
    (call $list_get)
    (global.set $current)
    (global.get $current)
    (i32.const 2)
    (drop) (drop)
    (call $list_create (i32.const 0))
    (i32.const 0)
    (i32.eq)
    (if
      (then
    (global.get $result)
    (call $list_create (i32.const 1))
    (local.set $temp)
    (global.get $current)
    (i32.const 2)
    (drop) (drop)
    (call $list_create (i32.const 0))
    (local.set $elem_val)
    (local.get $temp)
    (i32.const 0)
    (local.get $elem_val)
    (call $list_set)
    (local.get $temp)
    (call $list_concat)
    (global.set $result)
      )
      (else
    (global.get $result)
    (call $list_create (i32.const 1))
    (local.set $temp)
    (global.get $current)
    (local.set $elem_val)
    (local.get $temp)
    (i32.const 0)
    (local.get $elem_val)
    (call $list_set)
    (local.get $temp)
    (call $list_concat)
    (global.set $result)
      )
    )
    (global.get $i)
    (i32.const 1)
    (i32.add)
    (global.set $i)
        (br $until_start_0)
      )
    )
    (global.get $result)
    (return)
  )
  (func $main (export "main")
    (local $temp i32)
    (local $temp_list i32)
